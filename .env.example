## Copy this file to `.env` and fill in the values.  

# Token used for verifying webhook calls from the bank to the agent
SERVICE_TOKEN=devtoken

# Gemini API key (https://makersuite.google.com/app/apikey)
GEMINI_API_KEY = <GEMINI_API_KEY>

# Ollama settings (LLM used to paraphrase final answers)
# OLLAMA_URL=http://ollama:11434/api/generate
# OLLAMA_MODEL=llama3:8b

# Redis configuration for chat history
REDIS_HOST=redis
REDIS_PORT= <REDIS_PORT>
REDIS_PASS= <REDIS_PASS>

# Milvus configuration (used by RAG service)
MILVUS_HOST=localhost
MILVUS_PORT=19530

# Optional: default phone used by the UI
DEFAULT_PHONE=demo:thao